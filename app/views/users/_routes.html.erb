<div id='userRoutes'>
	<div class='searchImageWrapper'>
		<div class='searchImage'></div>
	</div>
	<h3 class='text-center searchBarHeader'>Let's look for rocks to climb, shall we?</h3>
	<div class='searchBar'>
		<%= form_tag search_path, :id => 'searchForm' do %>
			<div class='inline-block searchInput'>
				<%= text_field_tag 'search', nil,  placeholder: 'search routes', :class => 'searchBar' %>
			</div>
			<div class='inline-block  searchButton'>
				<i class="fa fa-search"></i>
			</div>
			<div class='hidden'>
			  <%= submit_tag 'search' %>
		  </div>
		<% end %>
	</div>
	<table id='searchResults' class='inline-block searchResults'>
		<thead>
			<td>name</td>
			<td>type</td>
			<td>rating</td>
			<td>height</td>
			<td>pitches</td>
			<td>more info</td>
		</thead>
		<tbody></tbody>
	</table>
</div>